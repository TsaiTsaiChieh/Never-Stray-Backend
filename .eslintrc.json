{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "new-cap": [
      "error",
      {
        "capIsNewExceptions": [
          "Router",
          "Entity",
          "Index",
          "PrimaryGeneratedColumn",
          "Column",
          "CreateDateColumn",
          "UpdateDateColumn",
          "Controller",
          "Get",
          "Req",
          "Res",
          "Unique",
          "Not",
          "IsNull"
        ]
      }
    ],
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "_"
      }
    ],
    "indent": [
      "off",
      2
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "_"
      }
    ],
    "camelcase": [
      "error",
      {
        "ignoreDestructuring": true,
        "ignoreImports": true,
        "ignoreGlobals": true,
        "allow": [
          "sub_id",
          "accept_num",
          "area_id",
          "created_at",
          "updated_at"
        ]
      }
    ]
  }
}